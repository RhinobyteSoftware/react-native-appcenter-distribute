{
	"extends": [
		"tslint-eslint-rules",
		"tslint-react"
	],
	"linter-options": {
		"exclude": []
	},
	"rules": {
		"adjacent-overload-signatures": true,
		"array-bracket-spacing": [true, "never"],
		"await-promise": true,
		"ban": [true, "eval", "alert", "confirm", "prompt"],
		"ban-comma-operator": true,
		"block-spacing": [true, "always"],
		"curly": true,
		"forin": true,
		"indent": [true, "tabs"],
		"interface-name": [true, "always-prefix"],
		"interface-over-type-literal": true,
		"label-position": true,
		"member-access": true,
		"new-parens": true,
		"no-any": true,
		"no-arg": true,
		"no-conditional-assignment": true,
		"no-constant-condition": true,
		"no-construct": true,
		"no-control-regex": true,
		"no-debugger": true,
		"no-default-export": true,
		"no-duplicate-case": true,
		"no-duplicate-imports": true,
		"no-duplicate-super": true,
		"no-duplicate-switch-case": true,
		"no-duplicate-variable": [true, "check-parameters"],
		"no-dynamic-delete": true,
		"no-empty": true,
		"no-empty-character-class": true,
		"no-eval": true,
		"no-ex-assign": true,
		"no-extra-boolean-cast": true,
		"no-extra-semi": true,
		"no-for-in-array": true,
		"no-inner-declarations": [true, "both"],
		"no-invalid-template-strings": true,
		"no-invalid-this": true,
		"no-invalid-regexp": true,
		"no-misused-new": true,
		"no-multi-spaces": true,
		"no-namespace": true,
		"no-object-literal-type-assertion": true,
		"no-reference": true,
		"no-regex-spaces": true,
		"no-return-await": true,
		"no-shadowed-variable": true,
		"no-string-throw": true,
		"no-switch-case-fall-through": true,
		"no-trailing-whitespace": true,
		"no-unbound-method": true,
		"no-unexpected-multiline": true,
		"no-unnecessary-type-assertion": true,
		"no-unsafe-finally": true,
		"no-unused-expression": true,
		"no-var-keyword": true,
		"object-curly-spacing": [true, "always"],
		"object-literal-key-quotes": [true, "consistent-as-needed"],
		"object-literal-shorthand": [true, "always"],
		"one-variable-per-declaration": [true, "ignore-for-loop"],
		"prefer-const": [true],
		"prefer-readonly": true,
		"quotemark": [true, "single", "jsx-single", "avoid-template", "avoid-escape"],
		"radix": true,
		"restrict-plus-operands": true,
		"semicolon": [true, "always", "strict-bound-class-methods"],
		"space-before-function-paren": [true, {
			"anonymous": "always",
			"asyncArrow": "always",
			"constructor": "never",
			"method": "never",
			"named": "never"
		}],
		"switch-default": true,
		"ter-arrow-spacing": [true],
		"ter-func-call-spacing": [true, "never"],
		"ter-indent": [true, "tab", {
			"SwitchCase": 1
		}],
		"ter-no-irregular-whitespace": [true],
		"ter-no-sparse-arrays": [true],
		"ter-prefer-arrow-callback": [true],
		"trailing-comma": [true, { "multiline": "never", "singleline": "never" }],
		"triple-equals": true,
		"typedef": [true, "call-signature"],
		"typedef-whitespace": [true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			},
			{
				"call-signature": "onespace",
				"index-signature": "onespace",
				"parameter": "onespace",
				"property-declaration": "onespace",
				"variable-declaration": "onespace"
			}
		],
		"type-literal-delimiter": true,
		"unified-signatures": true,
		"use-isnan": true,
		"variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case", "allow-leading-underscore"],



		"arrow-parens": [false, "always"],
		"brace-style": false,
		"comment-format": [false, "check-space"],
		"handle-callback-err": false,
		"newline-before-return": false,
		"newline-per-chained-call": false,
		"no-consecutive-blank-lines": false,
		"no-floating-promises": false,
		"no-implicit-dependencies": false,
		"no-import-sideeffect": false,
		"no-magic-numbers": false,
		"no-unnecessary-class": false,
		"no-use-before-declare": false,
		"object-literal-sort-keys": [false, "ignore-case"],
		"ordered-imports": [false, {
			"import-sources-order": "case-insensitive",
			"named-imports-order": "case-insensitive"
		}],
		"promise-function-async": false,
		"sort-imports": false,
		"space-within-parens": false,



		"jsx-alignment": false,
		"jsx-ban-props": false,
		"jsx-boolean-value": false,
		"jsx-curly-spacing": [false, "never"],
		"jsx-equals-spacing": [true, "never"],
		"jsx-key": true,
		"jsx-no-bind": true,
		"jsx-no-lambda": false,
		"jsx-no-multiline-js": false,
		"jsx-no-string-ref": true,
		"jsx-use-translation-function": false,
		"jsx-self-close": true,
		"jsx-wrap-multiline": true
	}
}
